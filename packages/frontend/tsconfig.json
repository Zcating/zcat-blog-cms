{
  "include": [
    "**/*",
    "**/.server/**/*",
    "**/.client/**/*",
    ".react-router/types/**/*"
  ],
  "compilerOptions": {
    "lib": [
      "DOM",
      "DOM.Iterable",
      "ES2022"
    ],
    "types": [
      "node",
      "vite/client"
    ],
    "target": "ES2022",
    "module": "ES2022",
    "moduleResolution": "bundler",
    "jsx": "react-jsx",
    "rootDirs": [
      ".",
      "./.react-router/types"
    ],
    "baseUrl": ".",
    "paths": {
      "@cms/*": [
        "./app/*"
      ],
      "@cms/api": [
        "./app/api"
      ],
      "@cms/api/*": [
        "./app/api/*"
      ],
      "@cms/components": [
        "./app/components"
      ],
      "@cms/components/*": [
        "./app/components/*"
      ],
      "@cms/modules": [
        "./app/modules"
      ],
      "@cms/modules/*": [
        "./app/modules/*"
      ],
    },
    "esModuleInterop": true,
    "verbatimModuleSyntax": true,
    "noEmit": true,
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "strict": true
  }
}