{
  "name": "@zcat-cms/z-components",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "main": "./dist/cjs/index.cjs",
  "module": "./dist/esm/index.js",
  "types": "./dist/types/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.cjs"
    },
    "./ui": {
      "types": "./dist/types/components/ui/index.d.ts",
      "import": "./dist/esm/components/ui/index.js",
      "require": "./dist/cjs/components/ui/index.cjs"
    },
    "./ui/*": {
      "types": "./dist/types/components/ui/*.d.ts",
      "import": "./dist/esm/components/ui/*.js",
      "require": "./dist/cjs/components/ui/*.cjs"
    },
    "./hooks": {
      "types": "./dist/types/components/hooks/index.d.ts",
      "import": "./dist/esm/components/hooks/index.js",
      "require": "./dist/cjs/components/hooks/index.cjs"
    },
    "./hooks/*": {
      "types": "./dist/types/components/hooks/*.d.ts",
      "import": "./dist/esm/components/hooks/*.js",
      "require": "./dist/cjs/components/hooks/*.cjs"
    },
    "./lib/*": {
      "types": "./dist/types/components/lib/*.d.ts",
      "import": "./dist/esm/components/lib/*.js",
      "require": "./dist/cjs/components/lib/*.cjs"
    },
    "./design": {
      "types": "./dist/types/design/index.d.ts",
      "import": "./dist/esm/design/index.js",
      "require": "./dist/cjs/design/index.cjs"
    },
    "./design/*": {
      "types": "./dist/types/design/*.d.ts",
      "import": "./dist/esm/design/*.js",
      "require": "./dist/cjs/design/*.cjs"
    },
    "./style.css": "./dist/style.css"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite",
    "build:types": "tsc -p tsconfig.build.json",
    "build": "pnpm run clean && pnpm run build:types && rollup -c",
    "typecheck": "tsc -p tsconfig.app.json --noEmit",
    "lint": "eslint .",
    "preview": "vite preview"
  },
  "peerDependencies": {
    "react": "^19.2.0",
    "react-dom": "^19.2.0"
  },
  "dependencies": {
    "dayjs": "^1.11.13",
    "@hookform/resolvers": "^5.2.1",
    "@radix-ui/react-avatar": "^1.1.2",
    "@radix-ui/react-label": "^2.1.8",
    "@radix-ui/react-navigation-menu": "^1.2.14",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-slot": "^1.2.4",
    "@tailwindcss/vite": "^4.1.10",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "lucide-react": "^0.515.0",
    "react-day-picker": "^9.13.0",
    "react-hook-form": "^7.58.1",
    "tailwind-merge": "^3.3.1",
    "tailwind-variants": "^3.2.2",
    "tailwindcss": "^4.1.10",
    "zod": "^4.0.17"
  },
  "devDependencies": {
    "@rollup/plugin-alias": "^5.1.1",
    "@rollup/plugin-commonjs": "^28.0.3",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^16.0.0",
    "@rollup/plugin-typescript": "^12.1.2",
    "@tailwindcss/postcss": "^4.1.10",
    "@eslint/js": "^9.39.1",
    "@types/node": "^24.10.4",
    "@types/react": "^19.2.5",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.1",
    "eslint": "^9.39.1",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "globals": "^16.5.0",
    "postcss": "^8.5.6",
    "postcss-import": "^16.1.0",
    "autoprefixer": "^10.4.21",
    "rollup": "^4.46.0",
    "rollup-plugin-postcss": "^4.0.2",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.46.4"
  }
}
